<div class="container-fluid mt-5">
      <form class="contact">
        <div class="row">
          <div class="form-group col-md-5 offset-1">
            <label for="input-firstname">Prénom</label>
            <input type="text" class="form-control" id="input-firstname" placeholder="Prénom" required>
          </div>
          <div class="form-group col-md-5">
            <label for="input-lastname">Nom</label>
            <input type="text" class="form-control" id="input-lastname" placeholder="Nom" required>
          </div>
        </div>
        <div class="row">
          <div class="form-group col-md-5 offset-1">
            <label for="input-phone">Téléphone</label>
            <input type="text" class="form-control" id="input-phone" placeholder="Nom">
          </div>
          <div class="form-group col-md-5">
            <label for="input-email">Email</label>
            <input type="email" class="form-control" id="input-email" placeholder="email@example.com" required>
          </div>
        </div>
        <div class="row">
          <div class="form-group col-md-10 offset-1">
            <label for="objet">Objet</label>
            <select class="custom-select" id="objet" required>
              <option selected>Demande de devis</option>
              <option selected>Demande d'information</option>
              <option selected>Prise de contact professionnelle</option>
              <option selected disabled>Choisir l'objet de votre demande...</option>
            </select>
          </div>
        </div>
        <div class="form-group col-md-10 offset-1">
          <label for="input-projet">Décrivez nous votre demande</label>
          <textarea class="form-control" id="input-projet" rows="5"></textarea>
        </div>
        <div class="text-center">
          <button class="btn btn-primary" type="submit">Envoyer</button>
        </div>
      </form>
</div>
